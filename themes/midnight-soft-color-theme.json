  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6c7086",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#89b4fa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.bitwise"
      ],
      "settings": {
        "foreground": "#89b4fa"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#f5c2e7"
      }
    },
    {
      "name": "String Quotes",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#f5c2e7"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language.numeric"],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.parameter", "variable.other"],
      "settings": {
        "foreground": "#b4befe"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#b4befe",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#b4befe",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type", "storage.type.primitive"],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name", "entity.name.tag"],
      "settings": {
        "foreground": "#b4befe"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#89b4fa"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#b4befe"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#b4befe"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#89b4fa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#b4befe",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#b4befe",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#89b4fa"
      }
    },
    {
      "name": "Python Decorators",
      "scope": ["entity.name.function.decorator.python"],
      "settings": {
        "foreground": "#b4befe",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python Self",
      "scope": ["variable.language.special.self.python"],
      "settings": {
        "foreground": "#89b4fa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JavaScript/TypeScript Keywords",
      "scope": ["keyword.control.flow.js", "keyword.control.flow.ts"],
      "settings": {
        "foreground": "#89b4fa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JavaScript/TypeScript This",
      "scope": ["variable.language.this.js", "variable.language.this.ts"],
      "settings": {
        "foreground": "#89b4fa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f38ba8",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Error",
      "scope": ["message.error"],
      "settings": {
        "foreground": "#f38ba8"
      }
    },
    {
      "name": "Warning",
      "scope": ["message.warning"],
      "settings": {
        "foreground": "#fab387"
      }
    },
    {
      "name": "Info",
      "scope": ["message.info"],
      "settings": {
        "foreground": "#89b4fa"
      }
    }
  ]
}

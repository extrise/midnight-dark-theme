  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6b7280",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#10b981",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.bitwise"
      ],
      "settings": {
        "foreground": "#10b981"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#84cc16"
      }
    },
    {
      "name": "String Quotes",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#84cc16"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language.numeric"],
      "settings": {
        "foreground": "#22c55e"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#22c55e"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.parameter", "variable.other"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#7dd3fc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#7dd3fc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type", "storage.type.primitive"],
      "settings": {
        "foreground": "#22c55e"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name", "entity.name.tag"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#10b981"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#22c55e"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#10b981",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#7dd3fc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#7dd3fc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#22c55e"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#10b981"
      }
    },
    {
      "name": "Python Decorators",
      "scope": ["entity.name.function.decorator.python"],
      "settings": {
        "foreground": "#7dd3fc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python Self",
      "scope": ["variable.language.special.self.python"],
      "settings": {
        "foreground": "#10b981",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JavaScript/TypeScript Keywords",
      "scope": ["keyword.control.flow.js", "keyword.control.flow.ts"],
      "settings": {
        "foreground": "#10b981",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JavaScript/TypeScript This",
      "scope": ["variable.language.this.js", "variable.language.this.ts"],
      "settings": {
        "foreground": "#10b981",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f87171",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Error",
      "scope": ["message.error"],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Warning",
      "scope": ["message.warning"],
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "Info",
      "scope": ["message.info"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    }
  ]
}

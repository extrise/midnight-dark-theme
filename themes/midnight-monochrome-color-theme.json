  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#b3b3b3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.bitwise"
      ],
      "settings": {
        "foreground": "#b3b3b3"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "name": "String Quotes",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language.numeric"],
      "settings": {
        "foreground": "#d9d9d9"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#d9d9d9"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.parameter", "variable.other"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type", "storage.type.primitive"],
      "settings": {
        "foreground": "#d9d9d9"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name", "entity.name.tag"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#b3b3b3"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#d9d9d9"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#b3b3b3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#d9d9d9"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#b3b3b3"
      }
    },
    {
      "name": "Python Decorators",
      "scope": ["entity.name.function.decorator.python"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python Self",
      "scope": ["variable.language.special.self.python"],
      "settings": {
        "foreground": "#b3b3b3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JavaScript/TypeScript Keywords",
      "scope": ["keyword.control.flow.js", "keyword.control.flow.ts"],
      "settings": {
        "foreground": "#b3b3b3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JavaScript/TypeScript This",
      "scope": ["variable.language.this.js", "variable.language.this.ts"],
      "settings": {
        "foreground": "#b3b3b3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#cccccc",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Error",
      "scope": ["message.error"],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Warning",
      "scope": ["message.warning"],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Info",
      "scope": ["message.info"],
      "settings": {
        "foreground": "#b3b3b3"
      }
    }
  ]
}

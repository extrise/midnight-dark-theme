  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#64748b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#38bdf8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.bitwise"
      ],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#22d3ee"
      }
    },
    {
      "name": "String Quotes",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#22d3ee"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language.numeric"],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.parameter", "variable.other"],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#60a5fa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#60a5fa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type", "storage.type.primitive"],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name", "entity.name.tag"],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#38bdf8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#60a5fa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#60a5fa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "Python Decorators",
      "scope": ["entity.name.function.decorator.python"],
      "settings": {
        "foreground": "#60a5fa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python Self",
      "scope": ["variable.language.special.self.python"],
      "settings": {
        "foreground": "#38bdf8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JavaScript/TypeScript Keywords",
      "scope": ["keyword.control.flow.js", "keyword.control.flow.ts"],
      "settings": {
        "foreground": "#38bdf8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JavaScript/TypeScript This",
      "scope": ["variable.language.this.js", "variable.language.this.ts"],
      "settings": {
        "foreground": "#38bdf8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f87171",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Error",
      "scope": ["message.error"],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Warning",
      "scope": ["message.warning"],
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "Info",
      "scope": ["message.info"],
      "settings": {
        "foreground": "#38bdf8"
      }
    }
  ]
}
